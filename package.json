  "bugs": {
    "url": "https://github.com/digital-land/data-manager/issues"
  },
  "dependencies": {
    "accessible-autocomplete": "^2.0.4",
    "govuk-frontend": "^5.9.0"
  },
  "devDependencies": {
    "@planning-data/digital-land-frontend": "^0.7.0",
    "npm-run-all": "^4.1.5",
    "nps": "^5.10.0",
    "standardx": "^7.0.0"
  },
  "engines": {
    "node": "16.x"
  }
}
